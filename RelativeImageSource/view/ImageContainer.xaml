             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:RelativeImageSource.view"
             mc:Ignorable="d" 
             d:DesignHeight="111.089" d:DesignWidth="180.154">
